import java.util.Scanner;

class Jk {
    public static void main(String[] args) {
        StringBuilder sb = new StringBuilder("Hello");
        System.out.println(sb.hashCode());//HashCode: 2065951873
        sb.append(" World...!");
        System.out.println(sb.hashCode());//HashCode: 2065951873
        String str = new String("Max");
        System.out.println(str.hashCode());//HashCode: 77124
        str = "Min";
        System.out.println(str.hashCode());//HashCode: 77362

    }
}
